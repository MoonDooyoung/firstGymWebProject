<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="kr.co.firstgym.bmiresult.mapper.IBmiResultListMapper">
  	
  	<resultMap type="kr.co.firstgym.command.BmiResultVO" id="bmiResultVO"></resultMap>
  	
  	<!-- BMI리스트 가져오기 -->
  	<select id="getList" resultMap="bmiResultVO">
  		SELECT * FROM bmi_result
  		WHERE 
  			user_id = #{userId}
  		ORDER BY bmi_num DESC
  	</select>
  	
  </mapper>